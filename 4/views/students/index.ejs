<!DOCTYPE html>
<html>
<head>
  <title>Students</title>
</head>
<body>
  <h1>Students</h1>
  <a href="/students/new">Add New Student</a><br><br>
  <table border="1">
    <tr>
      <th>Name</th>
      <th>Age</th>
      <th>department</th>
      <th>Actions</th>
    </tr>
    <% students.forEach(student => { %>
      <tr>
        <td><%= student.name %></td>
        <td><%= student.age %></td>
        <td><%= student.department %></td>
        <td>
          <a href="/students/edit/<%= student._id %>">Edit</a> |
          <a href="/students/delete/<%= student._id %>">Delete</a>
        </td>
      </tr>
    <% }) %>
  </table>
</body>
</html>
